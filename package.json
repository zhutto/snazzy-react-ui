    "react-emotion": "^9.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0"
  },
  "peerDependencies": {
    "prop-types": "15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
